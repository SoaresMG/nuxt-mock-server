{
  "name": "nuxt-mock-server",
  "version": "0.0.1",
  "description": "",
  "author": "Leandro Soares",
  "license": "MIT",
  "type": "module",
  "repository": "https://github.com/SoaresMG/nuxt-mock-server",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "workspaces": [
    "playground"
  ],
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "build:prepare": "nuxt-module-build prepare",
    "build:prepack": "nuxt-module-build build",
    "lint": "eslint . --fix",
    "lint:ci": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.12.4",
    "consola": "^3.2.3",
    "h3": "^1.12.0"
  },
  "dependencies": {
    "superjson": "^2.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@faker-js/faker": "^8.4.1",
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/eslint-config": "^0.4.0",
    "@nuxt/kit": "^3.12.4",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.12.4",
    "@nuxt/test-utils": "^3.14.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@types/node": "^20.14.11",
    "consola": "^3.2.3",
    "eslint": "^9.8.0",
    "husky": "^9.1.4",
    "nuxt": "^3.12.4",
    "semantic-release": "^24.0.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
  },
  "packageManager": "yarn@4.4.0+sha256.5f228cb28f2edb97d8c3b667fb7b2fdcf06c46798e25ea889dad9e0b4bc2e2c1"
